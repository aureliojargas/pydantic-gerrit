{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ConfigInfo",
  "description": "Gerrit server configuration information",
  "type": "object",
  "properties": {
    "gerrit": {
      "type": "object",
      "description": "Gerrit configuration details",
      "properties": {
        "all_projects": {
          "$ref": "projects.json#/ProjectInfo",
          "description": "Project that serves as source for the default access rights"
        },
        "all_users": {
          "$ref": "projects.json#/ProjectInfo",
          "description": "Project that serves as source for default user preferences"
        }
      }
    }
  }
}
